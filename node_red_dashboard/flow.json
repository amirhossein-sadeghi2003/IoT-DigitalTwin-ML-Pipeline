    {
        "id": "7f7e57dfaa9bae76",
        "type": "function",
        "z": "855d2da34cd02ed6",
        "name": "update_switches (AI)",
        "func": "let message = msg.payload;\nif (typeof message !== 'string') return null;\n\nlet output = null;\n\nif (message.startsWith('heater=')) {\n  output = { payload: message.endsWith('1') ? 'heater=1' : 'heater=0', topic: 'heater' };\n} else if (message.startsWith('cooler=')) {\n  output = { payload: message.endsWith('1') ? 'cooler=1' : 'cooler=0', topic: 'cooler' };\n} else if (message.startsWith('flood=')) {\n  output = { payload: message.endsWith('1') ? 'flood=1' : 'flood=0', topic: 'flood' };\n} else if (message.startsWith('window=')) {\n  output = { payload: message.endsWith('1') ? 'window=1' : 'window=0', topic: 'window' };\n}\n\nif (output) {\n    return output;\n}\n\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "x": 390,
        "y": 740,
        "wires": [
            [
                "eb67317ec2ee72ea"
            ]
        ]
    },
