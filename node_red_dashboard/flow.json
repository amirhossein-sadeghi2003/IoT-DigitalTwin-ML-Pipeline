    {
        "id": "7f7e57dfaa9bae76",
        "type": "function",
        "z": "855d2da34cd02ed6",
        "name": "update_switches (AI)",
        "func": "let message = msg.payload;\nif (typeof message !== 'string') return [null, null];\n\nlet controlMsg = null;\nlet statusMsg = null;\n\nfunction make(outName) {\n  const isOn = message.trim().endsWith('=1');\n  controlMsg = { payload: `${outName}=${isOn ? 1 : 0}` };\n  statusMsg = { topic: outName, payload: isOn ? 1 : 0 };\n}\n\nif (message.startsWith('heater=')) make('heater');\nelse if (message.startsWith('cooler=')) make('cooler');\nelse if (message.startsWith('flood=')) make('flood');\nelse if (message.startsWith('window=')) make('window');\nelse return [null, null];\n\nreturn [controlMsg, statusMsg];\n",
        "outputs": 2,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 680,
        "y": 720,
        "wires": [
            [
                "eb67317ec2ee72ea"
            ],
            [
                "4ddb6b9d98ae4370"
            ]
        ]
